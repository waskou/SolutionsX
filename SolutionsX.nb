      RowBox[{"xc", "=", 
       RowBox[{
        RowBox[{"$olution", "[", 
         RowBox[{"$basis", ",", "$curved", ",", "$coordinate"}], "]"}], "//.",
         "ToXCoord"}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"ds2", "=", 
       RowBox[{"$olution", "[", 
